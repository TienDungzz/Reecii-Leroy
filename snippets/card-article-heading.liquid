
<div class="card__heading dis-i-flex" {% if card_article.featured_media == null and settings.card_style == 'standard' %}id="title-{{ section_id }}-{{ card_article.id }}"{% endif %}>
    <a
        href="{{ card_article.url }}"
        id="StandardCardNoMediaLink-{{ section_id }}-{{ card_article.id }}"
        class="full-unstyled-link card-title-change"
        aria-labelledby="StandardCardNoMediaLink-{{ section_id }}-{{ card_article.id }} NoMediaStandardBadge-{{ section_id }}-{{ card_article.id }}"
    >
        {% render 'text', fallback_text: card_article.title, block: block, is_card_heading: true %}
    </a>
</div>