{{ 'base-3.css' | asset_url | stylesheet_tag: preload: true }}
{{ 'base-1.css' | asset_url | stylesheet_tag: preload: true }}
{{ 'base-2.css' | asset_url | stylesheet_tag: preload: true }}
{{ 'base-5.css' | asset_url | stylesheet_tag: preload: true }}
{{ 'base-4.css' | asset_url | stylesheet_tag: preload: true }}
<link rel="stylesheet" href="{{ 'component-slider.css' | asset_url }}" media="print" onload="this.media='all'">

{%- if settings.show_countdown -%}
    <link rel="preload" href="{{ 'component-card-countdown.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
{%- endif -%}

{%- if settings.show_calculator_free_shipping_message -%}
    <link rel="preload" href="{{ 'component-calculator-shipping.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
{%- endif -%}

{%- if settings.swatch != blank -%}
    <link rel="preload" href="{{ 'component-swatch.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
{%- endif -%}

{%- if localization.available_countries.size > 1 or localization.available_languages.size > 1 -%}
    <link rel="preload" href="{{ 'component-localization-form.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
{%- endif -%}

{%- if settings.predictive_search_enabled -%}
    <link rel="preload" href="{{ 'component-predictive-search.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
{%- endif -%}

<link rel="preload" href="{{ 'component-card.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<link rel="preload" href="{{ 'component-price.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<link rel="preload" href="{{ 'component-cart-items.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">

<link rel="preload" href="{{ 'animation.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">

{% comment %} Fallback for browsers that don't support preload {% endcomment %}
<noscript>
    {%- if settings.show_countdown -%}
        <link rel="stylesheet" href="{{ 'component-card-countdown.css' | asset_url }}">
    {%- endif -%}
    {%- if settings.show_calculator_free_shipping_message -%}
        <link rel="stylesheet" href="{{ 'component-calculator-shipping.css' | asset_url }}">
    {%- endif -%}
    {%- if settings.swatch != blank -%}
        <link rel="stylesheet" href="{{ 'component-swatch.css' | asset_url }}">
    {%- endif -%}
    {%- if localization.available_countries.size > 1 or localization.available_languages.size > 1 -%}
        <link rel="stylesheet" href="{{ 'component-localization-form.css' | asset_url }}">
    {%- endif -%}
    {%- if settings.predictive_search_enabled -%}
        <link rel="stylesheet" href="{{ 'component-predictive-search.css' | asset_url }}">
    {%- endif -%}
    <link rel="stylesheet" href="{{ 'component-card.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'component-price.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'component-cart-items.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'animation.css' | asset_url }}">
</noscript>
