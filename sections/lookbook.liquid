{{ 'lookbook.css' | asset_url | stylesheet_tag: preload: true }}

{% capture children %}
  {%- content_for 'block', type: '_product-list-content', id: 'static-header' -%}

  {% content_for 'block', type: '_lookbook', id: 'static-lookbook' %}

  <div
    class="section-resource-list__content"
    style="--horizontal-alignment: {{ section.settings.horizontal_alignment }};"
  >
    {%- content_for 'blocks' -%}
  </div>
{% endcapture %}

{% render 'section', section: section, children: children, is_lookbook: true %}

