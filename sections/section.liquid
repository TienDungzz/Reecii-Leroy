                "label": "Submit",
                "style_class": "button",
                "width": "fit-content",
                "custom_width": 100,
                "width_mobile": "fit-content",
                "custom_width_mobile": 100
              }
            }
          },
          "block_order": []
        }
      },
      "block_order": ["text", "contact_form"],
      "settings": {
        "content_direction": "column",
        "content_direction_mobile": "column",
        "horizontal_alignment": "flex-start",
